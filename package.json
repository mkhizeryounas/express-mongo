{
  "name": "zigzag",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "heroku-prebuild": "NPM_CONFIG_PRODUCTION=false npm install",
    "postdeploy": "npm run clean && npm run -s build",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r @babel/register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest",
    "precommit-msg": "echo 'ðŸš€ Pre-commit checks...' && exit 0",
    "lint": "eslint . --max-warnings=0 --report-unused-disable-directives && echo 'âœ¨ Linter passed!'",
    "format": "pretty-quick --staged"
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "format"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=12.22.0",
    "npm": ">=6.4.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "aws-sdk": "^2.1046.0",
    "axios": "^0.21.4",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "~3.1.7",
    "express": "~4.16.0",
    "express-recaptcha": "^5.1.0",
    "express-validation": "^1.0.2",
    "http-errors": "~1.6.2",
    "http-status-codes": "^1.3.0",
