{
  "name": "zigzag",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run clean && npm run -s build",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest",
    "precommit-msg": "echo 'ðŸš€ Pre-commit checks...' && exit 0",
    "lint": "eslint . --max-warnings=0 --report-unused-disable-directives && echo 'âœ¨ Linter passed!'",
    "format": "pretty-quick --staged"
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "format"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=10.15.1",
    "npm": ">=6.4.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "express-validation": "^1.0.2",
    "http-errors": "~1.6.2",
    "http-status-codes": "^1.3.0",
    "joi": "^14.3.1",
    "joi-objectid": "^4.0.2",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.10.5",
    "mongoose-paginate-v2": "^1.3.17",
    "morgan": "~1.9.0",
    "sha256": "^0.2.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-register": "^6.26.0",
    "dotenv": "^7.0.0",
    "eslint": "^5.15.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-watch": "^5.0.1",
    "jest": "^24.5.0",
    "kill-port": "^1.6.1",
    "nodemon": "^1.19.4",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "supertest": "^4.0.2"
  }
}
