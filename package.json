    "joi-objectid": "^4.0.2",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.10.5",
    "mongoose-paginate-v2": "^1.3.17",
    "morgan": "~1.9.0",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "sha256": "^0.2.0",
    "uuid4": "^2.0.2",
    "validator": "^13.7.0",
    "dotenv": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
